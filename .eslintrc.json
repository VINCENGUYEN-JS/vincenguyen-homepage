{
  "root": true,
  "extends": ["next", "plugin:@typescript-eslint/recommended", "prettier"],
  "rules": {
    "no-unused-vars": [
      "error",
      {
        "argsIgnorePattern": "^_",
        "varsIgnorePattern": "^_"
      }
    ],
    "prefer-const": "error",
    "@typescript-eslint/no-unused-vars": "error",
    // "@typescript-eslint/no-explicit-any": "error",
    "react/display-name": 0
  }
}
